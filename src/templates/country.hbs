{{#each this}}
<div>
    <h2 class="country-name">{{name}}</h2>
</div>
<div class="container">


    <div class="flag-block">
        <img src="{{flag}}" alt="{{name}} " class="flag">
    </div>
    <div class="description-block">


        <p><b>Population:</b> {{population}}</p>
        <p><b>Capital: </b>{{capital}}</p>
        <p><b>Languages:</p></b>
        <ul>
            {{#each languages}}
            <li>{{name}}</li>
            {{/each}}
        </ul>
    </div>
</div>
{{/each}}